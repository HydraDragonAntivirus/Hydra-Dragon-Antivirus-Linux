rule win_lu0bot_loader {

    meta:
        author      = "Johannes Bader @viql"
        date        = "2023-03-08"
        description = "detects the loader of the Lu0bot malware"
        tlp         = "TLP:WHITE"
        version     = "v1.0"
        hash_md5    = "c5eb9c6ded323a8db7eb739e514bb46c"
